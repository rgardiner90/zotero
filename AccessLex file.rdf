        <bib:authors>
            <rdf:Seq>
                <rdf:li>
                    <foaf:Person>
                        <foaf:surname>Florio</foaf:surname>
                        <foaf:givenname>Cassandra</foaf:givenname>
                    </foaf:Person>
                </rdf:li>
                <rdf:li>
                    <foaf:Person>
                        <foaf:surname>Hoffman</foaf:surname>
                        <foaf:givenname>Steven</foaf:givenname>
                    </foaf:Person>
